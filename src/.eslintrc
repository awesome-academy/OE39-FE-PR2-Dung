{
  "env": {
      "browser": true,
      "es6": true
  },
  "extends": ["react-app", "prettier"],
  "plugins": ["react", "prettier"],
  "parserOptions": {
      "ecmaVersion": 2018
  },
  "rules": {
      "prettier/prettier": [
          "warn",
          {
            "printWidth": 80,
            "trailingComma": "es5",
            "semi": true,
            "doubleQuote": true,
            "no-mixed-spaces-and-tabs": ["error", "smart-tabs"]
          }
      ],
      "no-unused-vars": ["warn", { "vars": "all", "args": "after-used", "ignoreRestSiblings": false }]
  }
}